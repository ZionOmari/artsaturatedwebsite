{
  "name": "art-saturated-website",
  "version": "2.0.0",
  "private": true,
  "description": "Modern art portfolio website with advanced grayscale effects and immersive visual experiences",
  "keywords": ["art", "portfolio", "grayscale", "react", "typescript", "tailwind"],
  "dependencies": {
    "@types/node": "^18.19.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "framer-motion": "^10.16.0",
    "react-intersection-observer": "^9.5.0",
    "react-lazyload": "^3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx,css,md}"
